{
    "name": "awssat/tailwindo",
    "description": "Convert Bootstrap CSS to Tailwind CSS",
    "keywords": [
        "CSS",
        "Tailwind",
        "Bootstrap"
    ],
    "homepage": "https://github.com/awssat/tailwindo",
    "license": "MIT",
    "authors": [
        {
            "name": "Awssat",
            "email": "hello@awssat.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.2",
        "symfony/console": "^4.0|^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9|^10|^11"
    },
    "autoload": {
        "psr-4": {
            "Awssat\\Tailwindo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awssat\\Tailwindo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "bin": [
        "tailwindo"
    ]
}
